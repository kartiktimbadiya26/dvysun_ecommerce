{
      "routes": [
            {
                  "src": "/ajax/(.*)",
                  "dest": "/public/ajax/$1"
            },
            {
                  "src": "/assets/(.*)",
                  "dest": "/public/assets/$1"
            },
            {
                  "src": "/npm/(.*)",
                  "dest": "/public/npm/$1"
            },
            {
                  "src": "/(.*)",
                  "dest": "/api/index.php"
            }
      ]
}