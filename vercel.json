{
      "version": 2,
      "builds": [
            {
                  "src": "/index.php",
                  "use": "vercel-php@0.3.1"
            },
            {
                  "src": "/public/**",
                  "use": "@vercel/static"
            },
            {
                  "src": "/public/**",
                  "use": "@vercel/static"
            }
      ],
      "routes": [
            {
                  "src": "/(css|js)/(.*)",
                  "dest": "public/$1/$2"
            },
            {
                  "src": "/(.*)",
                  "dest": "/index.php"
            }
      ]
}